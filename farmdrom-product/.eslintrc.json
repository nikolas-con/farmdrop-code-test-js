{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "process": true
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "react/prop-types": 0
  },
  "overrides": [
      {
          "files": [
              "**/*.test.js"
          ],
          "env": {
              "jest": true
          }
      }
  ],
  "settings": {
      "react": {
          "version": "detect"
      }
  }
}